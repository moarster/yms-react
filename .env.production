# API Configuration
VITE_API_BASE_URL=https://api-yms.solvo.ru
VITE_APP_NAME="Carrier Portal"
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Platform for warehouse owners and logistics companies"

# Enable production optimizations
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CDN=true

# Authentication Configuration
VITE_AUTH_MODE=keycloak

# Keycloak Configuration
VITE_KEYCLOAK_URL=https://keycloak.solvo.ru
VITE_KEYCLOAK_REALM=camunda-platform
VITE_KEYCLOAK_CLIENT_ID=carrier-portal-frontend

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Build optimizations
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=10000